{
  "name": "hennig-common",
  "version": "1.1.2",
  "description": "Common JS code for projects",
  "main": "index.js",
  "files": [
    "src/components/*.vue",
    "src/lib/*.js",
    "index.js"
  ],
  "author": "Ivan Hennig",
  "license": "ISC",
  "peerDependencies": {
    "autonumeric": "^4.6.0",
    "inputmask": "^5.0.5",
    "jquery": "^3.5.1",
    "moment": "^2.29.1",
    "numeral": "^2.0.6"
  },
  "eslintConfig": {
    "root": true,
    "env": {
      "node": true
    },
    "extends": [
      "plugin:vue/essential",
      "@vue/standard"
    ],
    "parserOptions": {
      "ecmaVersion": 2020
    },
    "rules": {
      "indent": [
        "error",
        2
      ],
      "vue/html-indent": [
        "error",
        2
      ],
      "vue/order-in-components": "error",
      "curly": "error",
      "camelcase": "off",
      "no-debugger": "off",
      "no-unused-vars": "off",
      "no-prototype-builtins": "off",
      "no-new": "off"
    }
  },
  "devDependencies": {
    "@vue/eslint-config-standard": "^6.0.0",
    "eslint-plugin-standard": "^5.0.0",
    "eslint-plugin-vue": "^7.11.0"
  },
  "dependencies": {
    "@kyvg/vue3-notification": "^2.3.2"
  }
}
